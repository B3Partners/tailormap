{
  "name": "tailormap-components-config",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-watch serve",
    "serve": "ng serve",
    "build": "ng build",
    "install-all": "npm run merge-dependencies && npm install",
    "build-prod": "ng build --configuration=production --output-hashing=none",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "merge-dependencies": "node bin/merge-dependencies.js"
  },
  "watch": {
    "serve": {
      "verbose": true,
      "patterns": [
        "../../tailormap-frontend/projects"
      ],
      "extensions": "ts,css,html"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/elements": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/material": "~12.1.1",
    "@angular/material-moment-adapter": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@tailormap/config": "0.0.17",
    "@tailormap/core-components": "0.0.15",
    "@tailormap/api": "0.0.5",
    "@tailormap/shared": "0.0.18",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-watch": "0.11.0",
    "typescript": "~4.3.2"
  }
}